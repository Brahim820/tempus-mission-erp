<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data>

    
			<!--											 -->
    		<!-- 			Vue Formulaire / Form			 -->
			<!--											 -->

    
	        <record model="ir.ui.view" id="globaldipfiliere_form_view">
				<field name="name">globaldipfiliere.form</field>
				<field name="model">inscrits.globaldipfiliere</field>
				<field name="arch" type="xml">
					<form string="Global Dipfiliere Form">
						<field name="cod_etab" colspan="4" />
						<field name="lib_etab" colspan="4" />
						<field name="cod_dip" colspan="4" />
						<field name="lib_dip" colspan="4" />
						<field name="cod_fil" colspan="4" />
						<field name="lib_fil" colspan="4" />
						<field name="annee" colspan="4" />
						<field name="nbr" colspan="4" />
					</form>
				</field>
			</record>
	
    
			<!--											 				-->
    		<!-- 				Vue Liste / Tree / Etab / Dip / Fil			-->
			<!--											 				-->


		 	<record model="ir.ui.view" id="globaldipfiliere_tree_view">
				<field name="name">globaldipfiliere.tree</field>
				<field name="model">inscrits.globaldipfiliere</field>
				<field name="arch" type="xml">
					<tree string="Global Dipfiliere Tree">
						<field name="cod_etab" />
						<field name="lib_etab" />
						<field name="cod_dip" />
						<field name="lib_dip" />
						<field name="cod_fil" />
						<field name="lib_fil" />
						<field name="annee" />
						<field name="nbr" />
					</tree>
				</field>
			</record>
			
    
			<!--											 				-->
    		<!-- 				Vue Liste / Tree / Etab / Prov 				-->
			<!--											 				-->


		 	<record model="ir.ui.view" id="globalprov_tree_view">
				<field name="name">globalprov.tree</field>
				<field name="model">inscrits.globalprov</field>
				<field name="arch" type="xml">
					<tree string="Global Prov Tree">
						<field name="cod_etab" />
						<field name="lib_etab" />
						<field name="cod_prov" />
						<field name="lib_prov" />
						<field name="annee" />
						<field name="nbr" />
					</tree>
				</field>
			</record>
			
    
			<!--											 -->
    		<!-- 					Vue Search	 			 -->
			<!--											 -->

		   <record id="globaldipfiliere_search_view" model="ir.ui.view">
	            <field name="name">globaldipfiliere.search</field>
	            <field name="model">inscrits.globaldipfiliere</field>
	            <field name="arch" type="xml">
	                <search string="Search">
						<field name="cod_etab" string="Code Etab" />
						<field name="lib_etab" string="Lib Etab" />
		                <separator/>
						<field name="cod_dip" string="Code Diplome" />
						<field name="lib_dip" string="Lib Diplome" />
		                <separator/>
						<field name="cod_fil" string="Code Filiere" />
						<field name="lib_fil" string="Lib Filiere" />
		                <!-- separator/>
						<field name="cod_prov" string="Code Province" />
						<field name="lib_prov" string="Lib Province" /-->
		                <separator/>
						<field name="annee" string="Annee" />
		                <group  string="Regrouper par :">
		              		<filter string="Etablissement"  context="{'group_by':'cod_etab'}"/>
		              		<filter string="Diplome"  context="{'group_by':'cod_dip'}"/>
		              		<filter string="Filiere"  context="{'group_by':'cod_fil'}"/>
		              		<filter string="Annee"  context="{'group_by':'annee'}"/>
		                </group>
	  				 </search>
	            </field>
        	</record>
	        
    
			<!--											 -->
    		<!-- 				Vue Graph / Etab			 -->
			<!--											 -->

			<record model="ir.ui.view" id="globaletab_graph_view">
				<field name="name">globaldipfiliere.graph</field>
				<field name="model">inscrits.globaldipfiliere</field>
				<field name="arch" type="xml">
					<graph string="Effectif global des inscrits par Etab" type="pie">
						<field name="cod_etab" />
						<field name="nbr" operator="+"/>
						
					</graph>
				</field>
			</record>
	        
	        
			<!--											 -->
    		<!-- 				Vue Graph / Diplome			 -->
			<!--											 -->

			<record model="ir.ui.view" id="globaldip_graph_view">
				<field name="name">globaldipfiliere.graph</field>
				<field name="model">inscrits.globaldipfiliere</field>
				<field name="arch" type="xml">
					<graph string="Effectif global des inscrits" type="pie">
						<field name="cod_dip"/>
						<field name="nbr" operator="+"/>
					</graph>
				</field>
			</record>


			<!--											 -->
    		<!-- 				Vue Graph / Filiere			 -->
			<!--											 -->

			<record model="ir.ui.view" id="globalfil_graph_view">
				<field name="name">globaldipfiliere.graph</field>
				<field name="model">inscrits.globaldipfiliere</field>
				<field name="arch" type="xml">
					<graph string="Effectif global des inscrits fil" type="bar" >
						<field name="lib_fil"/>
						<field name="nbr" operator="+"/>
					</graph>
				</field>
			</record>
	        
    
			<!--											 -->
    		<!-- 				Vue Graph / Province			 -->
			<!--											 -->

			<record model="ir.ui.view" id="globalprov_graph_view">
				<field name="name">globalprov.graph</field>
				<field name="model">inscrits.globalprov</field>
				<field name="arch" type="xml">
					<graph string="Effectif global des inscrits" type="bar">
						<field name="lib_prov"/>
						<field name="nbr" operator="+"/>
					</graph>
				</field>
			</record>


	       <record model="ir.actions.act_window" id="globaldipfiliere_data_list_action">
	            <field name="name">Effectif global des inscrits - données</field>
	            <field name="res_model">inscrits.globaldipfiliere</field>
	            <field name="view_type">form</field>
	            <field name="view_mode">tree</field>
	            <field name="context">{'search_default_instructor_filter':1}</field>
	        </record>
	        
	       <record model="ir.actions.act_window" id="globalprov_data_list_action">
	            <field name="name">Effectif des nouveaux inscrits - données</field>
	            <field name="res_model">inscrits.globalprov</field>
	            <field name="view_type">form</field>
	            <field name="view_mode">tree</field>
	            <field name="context">{'search_default_instructor_filter':1}</field>
	        </record>
	        
	       <record model="ir.actions.act_window" id="globaldipfiliere_etab_list_action">
	            <field name="name">Effectif global des inscrits par Etablissement</field>
	            <field name="res_model">inscrits.globaldipfiliere</field>
	            <field name="view_type">form</field>
	            <field name="view_mode">graph</field>
	            <field name="view_id" ref="globaletab_graph_view" />
	            <field name="context">{'search_default_instructor_filter':1}</field>
	        </record>
	        
	       <record model="ir.actions.act_window" id="globaldipfiliere_dip_list_action">
	            <field name="name">Effectif global des inscrits par Diplôme</field>
	            <field name="res_model">inscrits.globaldipfiliere</field>
	            <field name="view_type">form</field>
	            <field name="view_mode">graph</field>
	            <field name="view_id" ref="globaldip_graph_view" />
	            <field name="context">{'search_default_instructor_filter':1}</field>
	        </record>
	        
	       <record model="ir.actions.act_window" id="globaldipfiliere_fil_list_action">
	            <field name="name">Effectif global,des inscrits par Filière</field>
	            <field name="res_model">inscrits.globaldipfiliere</field>
	            <field name="view_type">form</field>
	            <field name="view_mode">graph</field>
	            <field name="view_id" ref="globalfil_graph_view" />
	            <field name="context">{'search_default_instructor_filter':1}</field>
	        </record>
	        
	       <record model="ir.actions.act_window" id="globalprov_prov_list_action">
	            <field name="name">Effectif global des inscrits par Province</field>
	            <field name="res_model">inscrits.globalprov</field>
	            <field name="view_type">form</field>
	            <field name="view_mode">graph</field>
	            <field name="view_id" ref="globalprov_graph_view" />
	            <field name="context">{'search_default_instructor_filter':1}</field>
	        </record>
	
	    	<menuitem id="main_apogee_menu" name="APOGEE" />
	    		<menuitem id="data_menu" name="Données de l'Effectif" parent="main_apogee_menu" sequence="11"/>
	    			<menuitem id="globaldipfiliere_data_menu" name="global par Filière" parent="data_menu" action="globaldipfiliere_data_list_action" sequence="12"/>
	    			<menuitem id="globalprov_data_menu" name="global par Province" parent="data_menu" action="globalprov_data_list_action" sequence="13"/>
	    		<menuitem id="global_menu" name="Effectif global des inscrits" parent="main_apogee_menu" sequence="1"/>
			    	<menuitem id="global_etab_menu" name="Par Etablissement" parent="global_menu" action="globaldipfiliere_etab_list_action" sequence="2"/>
			    	<menuitem id="global_dip_menu" name="Par Diplôme" parent="global_menu" action="globaldipfiliere_dip_list_action" sequence="3"/>
			    	<menuitem id="global_fil_menu" name="Par Filières" parent="global_menu" action="globaldipfiliere_fil_list_action" sequence="4"/>
			    	<menuitem id="global_prov_menu" name="Par Province" parent="global_menu" action="globalprov_prov_list_action" sequence="5"/>

    </data>
</openerp>